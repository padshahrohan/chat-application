{
  "name": "chat-application-using-rpc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/setenv.ts",
    "start": "npm run config -- --server_ip=%npm_config_server_ip% --isIterative=%npm_config_is_iterative% && ng serve --host %npm_config_server_ip%",
    "build": "npm run config -- --server_ip=%npm_config_server_ip% && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "protoc": "protoc --plugin=protoc-gen-ts=%CD%/node_modules/.bin/protoc-gen-ts.cmd --js_out=import_style=commonjs,binary:src/app/proto-gen --ts_out=service=grpc-web:src/app/proto-gen -I %CD%/src/proto/ %CD%/src/proto/*.proto"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^4.6.1",
    "bootstrap-icons": "^1.8.3",
    "google-protobuf": "^3.21.2",
    "protoc": "^1.0.4",
    "rxjs": "~7.5.0",
    "ts-protoc-gen": "^0.15.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.5",
    "@angular/cli": "~14.2.5",
    "@angular/compiler-cli": "^14.2.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "yargs": "^17.6.0"
  }
}
